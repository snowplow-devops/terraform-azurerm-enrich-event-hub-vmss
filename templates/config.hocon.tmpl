{
  "input": {
    "topicName": "${raw_topic_name}"
    "bootstrapServers": "${kafka_brokers}"

    "consumerConf": {"client.id":$${CONSUMER_CLIENT_ID},"group.id":"${raw_group_id}","security.protocol":"SASL_SSL","sasl.mechanism":"PLAIN","sasl.jaas.config":"org.apache.kafka.common.security.plain.PlainLoginModule required username=\"${raw_topic_kafka_username}\" password=\"${raw_topic_kafka_password}\";"}
  }
  "output": {
    "good": {
      "topicName": "${good_topic_name}"
      "bootstrapServers": "${kafka_brokers}"

      "producerConf": {"client.id":$${PRODUCER_ENRICHED_CLIENT_ID},"security.protocol":"SASL_SSL","sasl.mechanism":"PLAIN","sasl.jaas.config":"org.apache.kafka.common.security.plain.PlainLoginModule required username=\"${good_topic_kafka_username}\" password=\"${good_topic_kafka_password}\";"}
    }
    "bad": {
      "topicName": "${bad_topic_name}"
      "bootstrapServers": "${kafka_brokers}"

      "producerConf": {"client.id":$${PRODUCER_BAD_CLIENT_ID},"security.protocol":"SASL_SSL","sasl.mechanism":"PLAIN","sasl.jaas.config":"org.apache.kafka.common.security.plain.PlainLoginModule required username=\"${bad_topic_kafka_username}\" password=\"${bad_topic_kafka_password}\";"}
    }
  }

  "assetsUpdatePeriod": "${assets_update_period}"

  "monitoring": {
    "metrics": {
      "stdout": {
        "period": "60 seconds"
      }
    }
  }

  "telemetry": {
    "disable": ${telemetry_disable}
    "interval": 15 minutes
    "method": "POST"
    "collectorUri": "${telemetry_collector_uri}"
    "collectorPort": ${telemetry_collector_port}
    "secure": ${telemetry_secure}
    "userProvidedId": "${telemetry_user_provided_id}"
    "autoGeneratedId": "${telemetry_auto_gen_id}"
    "moduleName": "${telemetry_module_name}"
    "moduleVersion": "${telemetry_module_version}"
    "instanceId": $${INSTANCE_ID}
  }
}
